<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Cross Browser Testing" thread-count="2" parallel="tests">
	<test name="Chrome Test">
		<parameter name="browser" value="Chrome" />
		<classes>
			<class name="testCases.Test_Search_With_Terms" />
			<class name="testCases.Test_Search_WithOut_Terms" />
		</classes>
	</test> <!-- Test -->

	<test name="Firefox Test">
		<parameter name="browser" value="Firefox" />
		<classes>
			<class name="testCases.Test_Search_With_Terms" />
			<class name="testCases.Test_Search_WithOut_Terms" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->